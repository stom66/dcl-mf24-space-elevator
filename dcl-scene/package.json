{
  "name": "dcl-boilerplate",
  "version": "1.0.0",
  "description": "Decentraland blank boilerplate scene",
  "scripts": {
    "start": "sdk-commands start",
    "deploy": "sdk-commands deploy",
    "build": "sdk-commands build",
    "upgrade-sdk": "npm install --save-dev @dcl/sdk@latest",
    "upgrade-sdk:next": "npm install --save-dev @dcl/sdk@next"
  },
  "devDependencies": {
    "@dcl/asset-packs": "^1.17.0",
    "@dcl/js-runtime": "7.6.1",
    "@dcl/sdk": "^7.5.5"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=6.0.0"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "printWidth": 120,
    "trailingComma": "none"
  },
  "dependencies": {
    "@dcl/sdk-commands": "7.4.21",
    "dcl-ui-toolkit": "^0.10.0"
  },
  "bundledDependencies": [
    "dcl-ui-toolkit"
  ],
  "bundleDependencies": [
    "dcl-ui-toolkit"
  ]
